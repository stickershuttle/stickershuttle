# Frontend Environment Variables Template
# Copy this file to .env.local and update the values

# For local development (connecting to local backend on port 4000)
# NEXT_PUBLIC_API_URL=http://localhost:4000

# For local development (connecting to production Railway backend)
# NEXT_PUBLIC_API_URL=https://ss-beyond.up.railway.app

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Stripe Configuration
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key

# Cloudinary Configuration
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name

# PostHog Configuration (optional)
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_key
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com 